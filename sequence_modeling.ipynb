   "source": [
    "def generate_sequences(seq_length, num_samples=100):\n",
    "    torch.manual_seed(0)\n",
    "    sequences = torch.randn(num_samples, seq_length, 1)\n",
    "    targets = sequences[:, 0, 0]\n",
    "    return sequences, targets.unsqueeze(1)"
   ]
  },
